{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "name": "Article",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "titre": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "image": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "date_publication": {
      "type": "date",
      "required": true
    },
    "auteur": {
      "model": "auteur"
    },
    "categories": {
      "collection": "categorie",
      "via": "articles",
      "dominant": true
    },
    "sous_titre": {
      "type": "string"
    },
    "contenu_pdf": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "files"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    }
  }
}
